  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LAZONEDEV/medusa-product-availability-plugin"
  },
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa",
    "medusa-plugin",
    "medusa-product-availability-plugin",
    "medusa-product-storefront-plugin"
  ],
  "author": {
    "name": "LA ZONE",
    "url": "https://www.la-zone.io",
    "email": "admin@la-zone.io"
  },
  "contributors": [
    {
      "name": "Lewis ALLEDJI",
      "url": "https://github.com/LewisYann"
    },
    {
      "name": "Juste BOCOVO",
      "url": "https://github.com/BOCOVO"
    },
    {
      "name": "Valentino Houessou",
      "url": "https://github.com/Valentino-Houessou"
    },
    {
      "name": "Teneng Clifford",
      "url": "https://github.com/cliffordten"
    }
  ],
  "scripts": {
    "clean": "cross-env ./node_modules/.bin/rimraf dist",
    "build": "cross-env npm run clean && npm run build:server && npm run build:admin:bundle",
    "build:server": "cross-env npm run clean && tsc -p tsconfig.json && tsc-alias",
    "build:admin": "cross-env medusa-admin build",
    "build:admin:bundle": "cross-env medusa-admin bundle",
    "watch": "cross-env tsc --watch",
    "test": "cross-env jest",
    "seed": "cross-env medusa seed -f ./data/seed.json",
    "start": "cross-env npm run build && medusa start",
    "start:custom": "cross-env npm run build && node --preserve-symlinks --trace-warnings index.js",
    "dev": "concurrently \"tsc-alias -w\" \"cross-env npm run build:server && medusa develop\"",
    "migration:create": "yarn build:server && npx typeorm migration:generate -d datasource.js",
    "prepare": "husky install",
    "lint": "lint-staged",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "formik": "^2.4.5",
    "yup": "^1.3.3"
  },
  "peerDependencies": {
    "@medusajs/icons": "1.1.0",
    "@medusajs/medusa": "1.20.9",
    "@medusajs/ui": "2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-typescript": "^7.21.4",
    "@medusajs/admin": "7.1.16",
    "@medusajs/cache-inmemory": "^1.8.11",
    "@medusajs/cache-redis": "^1.9.2",
    "@medusajs/event-bus-local": "^1.9.9",
    "@medusajs/event-bus-redis": "^1.8.14",
    "@medusajs/file-local": "^1.0.4",
    "@medusajs/icons": "1.1.0",
    "@medusajs/medusa": "1.20.9",
    "@medusajs/medusa-cli": "^1.3.23",
    "@medusajs/ui": "2.3.0",
    "@medusajs/utils": "^1.11.11",
    "@swc/jest": "^0.2.36",
    "@tanstack/react-query": "4.22.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.13",
    "@types/node": "^17.0.8",
    "babel-preset-medusa-package": "^1.1.19",
    "body-parser": "^1.19.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^6.8.0",
    "express": "^4.17.2",
    "get-port": "^5.1.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "medusa-fulfillment-manual": "^1.1.41",
    "medusa-interfaces": "^1.3.10",
    "medusa-payment-manual": "^1.0.25",
    "medusa-test-utils": "1.1.45",
    "prettier": "3.2.0",
    "prism-react-renderer": "^2.0.4",
    "react": "^18.2.0",
    "react-router-dom": "^6.21.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "tsc-alias": "^1.8.8",
    "typeorm": "^0.3.16",
    "typescript": "^4.5.2"
  }
}
